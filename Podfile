  pod 'GooglePlaces'
  pod 'GooglePlacePicker'

  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Firebase/Core'	
  pod 'Firebase/Messaging'
  pod 'PayPal-iOS-SDK'
  pod 'SwiftySound'
  pod ‘SinchRTC’  
 

  # Pods for Tawsila

  target 'TawsilaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TawsilaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
